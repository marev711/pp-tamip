<definition>
    <write_namelist filename='indata.nml'>
        &amp;indata
          ccbt_level_in_var96 = 1,
          var96_file = '${RUN_FOLDER}/96.128.grb',
          surface_pressure_file = '${RUN_FOLDER}/134.128.grb',
          ccbt_outfile = '${RUN_FOLDER}/96cct.128.grb'
        /
    </write_namelist>

    <command_block>
        compute_ccbt.x
        cdo -R -r -f nc -t ecmwf copy 96cct.128.grb 96cct.128_tmp.nc
        cdo timselmean,4 96cct.128_tmp.nc 96cct.128.nc
    </command_block>

    <post_cdo_units> 
        Pa
     </post_cdo_units>

    <original_name>
        TSRC/2700
    </original_name>

</definition>

